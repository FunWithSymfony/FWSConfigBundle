{
    "name":         "fws/fws-config-bundle",
    "description":  "This bundle provides an integration of the KexConfig library",
    "keywords":     ["config"],
    "type":         "symfony-bundle",
    "license":      "MIT",
    "authors":      [
        {
            "name":     "Anthony Howell",
            "email":    "anthonyhowell@gmail.com"
        }
    ],
    "require":      {
        "kex/kex-config": "dev-master",
        "symfony/framework-bundle": "~2.3|~3.0"
    },
    "require-dev":  {
        "symfony/phpunit-bridge": "~2.7|~3.0",
        "symfony/expression-language": "~2.4|~3.0"
    },
    "autoload":     {
        "psr-4": { "FWS\\Bundle\\ConfigBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
